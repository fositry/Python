print('-' * 25)
print('SEQUÃŠNCIA FIBONACCI')
print('-' * 25)
termos = int(input('Quantos termos vc quer mostrar? '))
termo1 = 0
termo2 = 1
print('{} -> {} -> '.format(termo1, termo2), end='')
contador = 3
while contador <= termos:
    termo3 = termo1 + termo2
    print('{} -> '.format(termo3), end='')
    contador += 1
    termo1 = termo2
    termo2 = termo3
print('FIM')
