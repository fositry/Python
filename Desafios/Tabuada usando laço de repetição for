p = 0
n = int(input('Digite um número para ver sua tabuada: '))
print('_' * 12)
for p in range(1, 11):
    t = n * p
    print('{} x {:2} = {}'.format(n, p, t))
print('_' * 12)

n = int(input('Digite um número para ver sua tabuada: '))
for c in range(1, 11):
    print('{} x {:2} = {}'.format(n, c, n * c))
