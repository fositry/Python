contador = 0
while True:
    print('-' * 50)
    numero = int(input('Digite um número para ver sua tabuada: '))
    print('-' * 50)
    if numero < 0:
        break
    for contador in range(1, 11):
        print(f'{numero} x {contador} = {numero * contador}')
print('Programa encerrado. Até logo!')
